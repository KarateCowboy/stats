{
  "name": "vault-analytics",
  "version": "1.0.6",
  "description": "Vault analytics UI and data processors",
  "main": "index.js",
  "engines": {
    "node": "9.9.x",
    "npm": "5.6.x"
  },
  "stardard": {
    "globals": ["describe", "expect", "context", "it", "specify", "before", "beforeEach", "db" ] 
  },
  "scripts": {
    "ui-tests": "webpack-dev-server --config webpack.test.config.js",
    "heroku-postbuild": "webpack --config ./webpack.config.js --progress",
    "build": "webpack-cli",
    "start": "webpack-cli && node src/run.js",
    "selenium": "java -jar -Dwebdriver.chrome.driver=./test/features/support/chromedriver  ./test/features/support/selenium-server-standalone-3.12.0.jar",
    "phantom": "phantomjs --webdriver=4444",
    "selenium-ff": "java -jar -Dwebdriver.gecko.driver=./test/features/support/geckodriver ./test/features/support/selenium-server-standalone-3.12.0.jar",
    "cucumber": "./node_modules/.bin/cucumber-js ./test",
    "lint": "standard",
    "postinstall": "bower cache clean && bower install",
    "test": "tap test/*.js",
    "mocha": "mocha test/**/*.spec.js --exclude test/ui/**/*"
  },
  "author": "Brave Software",
  "license": "ISC",
